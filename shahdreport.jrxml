	<queryString language="SQL">
		<![CDATA[SELECT
    "name_of_department",
    MAX(number_of_product) AS number_of_product,
    MAX(employee_count) AS employee_count
FROM (
    -- Count of products for each department
    SELECT
        d."name_of_department",
        COUNT(c."dname") AS number_of_product,
        0 AS employee_count
    FROM
        project.department AS d
        LEFT JOIN project.contains AS c ON d."name_of_department" = c."dname"
    GROUP BY
        d."name_of_department"

    UNION

    -- Count of employees for each department
    SELECT
        d."name_of_department",
        0 AS number_of_product,
        COUNT(e."firstname") AS employee_count
    FROM
        project.department AS d
        JOIN project.employee AS e ON d."name_of_department" = e."name_of_department"
    WHERE
        d."name_of_department" <> '-'
    GROUP BY
        d."name_of_department"
    HAVING
        COUNT(e."firstname") IS NOT NULL
) AS subquery
GROUP BY
    "name_of_department"
HAVING
    MAX(number_of_product) > 0 OR MAX(employee_count) > 0]]>
	</queryString>
